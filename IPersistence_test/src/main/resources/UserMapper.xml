<mapper namespace="user">

    <select id="selectList" resultType="com.zhul.pojo.User">
        select * from user
    </select>

    <select id="selectOne" paramterType = "com.zhul.pojo.User" resultType="com.zhul.pojo.User">
        select * from user where id = #{id} and username = #{username}
    </select>



</mapper>